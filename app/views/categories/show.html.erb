<div class="expenses">
  <div>
    <%= render "layouts/header_back_categories", title: 'EXPENSES' %>

  </div>
  <div class="expenses-list">
  <% @category.expenses.order('created_at DESC').each do |expense| %>
    <%= render expense %>
  <% end %>
  </div>

  <%= link_to "New Expense", new_category_expense_path(@category) %>
</div>

